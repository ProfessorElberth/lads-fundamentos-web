<div class="form-row">
	<div class="form-group col-md-2">
		<label>Cep:</label>
		<input type="text" class="form-control" value="${meuEndereco.cep}" placeholder="Entre com o seu cep" name="cep">
	</div>
	<div class="form-group col-md-5">
		<label>Logradouro:</label>
		<input type="text" class="form-control" value="${meuEndereco.logradouro}" placeholder="Entre com o seu logradouro" name="logradouro">
	</div>
	<div class="form-group col-md-5">
		<label>Complemento:</label>
		<input type="text" class="form-control" value="${meuEndereco.complemento}" placeholder="Entre com o seu complemento" name="complemento">
	</div>
</div>

<div class="form-row">
	<div class="form-group col-md-4">
		<label>Bairro:</label>
		<input type="text" class="form-control" value="${meuEndereco.bairro}" placeholder="Entre com o seu bairro" name="bairro">
	</div>
	<div class="form-group col-md-6">
		<label>Localidade:</label>
		<input type="text" class="form-control" value="${meuEndereco.localidade}" placeholder="Entre com a sua localidade" name="localidade">
	</div>
	<div class="form-group col-md-2">
		<label>Unidade da Federação:</label>
		<input type="text" class="form-control" value="${meuEndereco.uf}" placeholder="Entre com a sua unidade da federação" name="uf">
	</div>
</div>